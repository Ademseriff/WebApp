@model Arac
@using WebApp.Models
@{
    ViewData["Title"] = "AracEkle";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form method="post" asp-controller="Arac" asp-action="AracEkle">
    <div class="form-group">
        <label for="AracMarka">Araç Marka</label>
        <input type="text" class="form-control"  asp-for="AracMarka">
    </div>

    <div class="form-group">
        <label for="AracModel">Araç Model</label>
        <input type="text" class="form-control"  asp-for="AracModel">
    </div>

    <div class="form-group">
        <label for="AracFiyat">Araç Fiyat</label>
        <input type="text" class="form-control"  asp-for="AracFiyat">
    </div>
    <button type="submit" class="btn btn-primary">Araç Ekle</button>
</form>

