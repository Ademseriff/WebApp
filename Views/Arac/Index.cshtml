@model List<Arac>
@using WebApp.Models
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-dark">
<thead>
    <tr>
        <th scope="col">Araç Id</th>
        <th scope="col">Araç Marka </th>
        <th scope="col">Araç Model</th>
        <th scope="col">Araç Fiyat</th>
    </tr>
</thead>
<tbody ">
@for (int i = 0; i < Model.Count; i++)
{
        <tr>
            <th scope="row">@Model[i].AracId</th>
            <td>@Model[i].AracMarka</td>
            <td>@Model[i].AracModel</td>
            <td>@Model[i].AracFiyat</td>
        @*<a asp-controller="Home" asp-action="Delete" asp-route-id="@Model[i].AdminId></a>*@
        </tr>

}

</tbody>
</table>

