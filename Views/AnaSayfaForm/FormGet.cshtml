@model List<Form>
@{
    ViewData["Title"] = "Kullanıcı Mesajları";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">ADI </th>
            <th scope="col">E mail Adresi </th>
            <th scope="col">Admin Şifre</th>

        </tr>
    </thead>
    <tbody ">
    @for (int i = 0; i < Model.Count; i++)
    {
            <tr>
                <th scope="row">@Model[i].Name</th>
                <td>@Model[i].Email</td>
                <td>@Model[i].Message</td>
          
            </tr>

    }

    </tbody>
</table>

